{
  "name": "@flex-development/tutils",
  "description": "TypeScript utilities",
  "version": "4.2.0",
  "keywords": [
    "type definitons",
    "typescript utilities",
    "typescript"
  ],
  "homepage": "https://github.com/flex-development/tutils",
  "repository": "git://github.com/flex-development/tutils.git",
  "bugs": {
    "url": "https://github.com/flex-development/tutils/issues"
  },
  "license": "BSD-3-Clause",
  "contributors": [
    {
      "name": "Lexus Drumgold",
      "url": "https://github.com/unicornware"
    }
  ],
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "cjs",
    "esm",
    "types",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./esm/tutils.mjs",
      "require": "./cjs/tutils.cjs"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.cjs"
    },
    "./cjs": "./cjs/index.cjs",
    "./cjs/*": "./cjs/*.cjs",
    "./esm": "./esm/index.mjs",
    "./esm/*": "./esm/*.mjs",
    "./guards": {
      "import": "./esm/guards/index.mjs",
      "require": "./cjs/guards/index.cjs"
    },
    "./guards/*": {
      "import": "./esm/guards/*.mjs",
      "require": "./cjs/guards/*.cjs"
    },
    "./package.json": "./package.json",
    "./types": {
      "import": "./esm/types/index.mjs",
      "require": "./cjs/types/index.cjs"
    },
    "./types/*": {
      "import": "./esm/types/*.mjs",
      "require": "./cjs/types/*.cjs"
    }
  },
  "main": "./cjs/tutils.min.cjs",
  "module": "./esm/tutils.min.mjs",
  "typings": "./types/index.d.ts",
  "scripts": {
    "clean:build": "rimraf ./(cjs|esm|types|*.tgz)",
    "clean:modules": "rimraf ./node_modules",
    "clean:yarn": "rimraf .yarn/cache .yarn/*.gz",
    "clean": "concurrently \"yarn:clean:*\"",
    "postinstall": "is-ci || chmod +x .husky/* && husky install",
    "check:dedupe": "yarn dedupe --check",
    "check:format": "prettier --check .",
    "check:install": "yarn dlx @yarnpkg/doctor .",
    "check:style": "eslint . --ext cjs,cts,md,mjs,mts,ts",
    "check:types": "tsc -p tsconfig.prod.json --noEmit",
    "check:upgrades": "yarn upgrade-interactive",
    "fix:format": "prettier --write .",
    "fix:style": "yarn check:style --fix --cache",
    "test": "bash ./tools/scripts/jest.sh",
    "build": "node ./tools/cli/build-pkg.ts",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "postpack": "toggle-scripts +postinstall",
    "release": "NODE_ENV=production node ./tools/cli/release.ts",
    "prepublishOnly": "toggle-scripts -prepack",
    "postpublish": "toggle-scripts +prepack"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/types": "13.2.0",
    "@flex-development/grease": "2.0.0",
    "@flex-development/log": "4.0.0-dev.0",
    "@flex-development/trext": "1.1.0",
    "@jest/globals": "27.2.5",
    "@jest/types": "27.2.5",
    "@tests/fixtures": "link:__tests__/__fixtures__",
    "@tests/utils": "link:__tests__/utils",
    "@tutils/enums": "link:src/enums",
    "@tutils/guards": "link:src/guards",
    "@tutils/types": "link:src/types",
    "@types/faker": "5.5.8",
    "@types/jest": "27.0.2",
    "@types/lodash.merge": "4.6.6",
    "@types/mvdan-sh": "0.5.1",
    "@types/node": "16.0.0",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.9",
    "@types/yargs": "17.0.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "@vates/toggle-scripts": "1.0.0",
    "@vercel/ncc": "0.31.1",
    "chalk": "4.1.2",
    "concurrently": "6.3.0",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.1",
    "eslint-plugin-jsdoc": "36.1.1",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-markdownlint": "0.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "eslint-plugin-unicorn": "37.0.1",
    "faker": "5.5.3",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest": "27.2.5",
    "jest-extended": "1.0.0",
    "jest-github-reporter": "1.0.2",
    "lint-staged": "11.2.3",
    "lodash.merge": "4.6.2",
    "prettier": "2.4.1",
    "prettier-plugin-sh": "0.7.1",
    "read-pkg": "7.0.0",
    "replace-in-file": "6.2.0",
    "resolve-tspaths": "0.1.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-jest": "27.0.6",
    "ts-node": "10.3.0",
    "tsc-prog": "2.2.1",
    "tsconfig": "7.0.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.5.0-beta",
    "yargs": "17.2.1"
  },
  "peerDependencies": {
    "@types/node": ">=14.0.0",
    "typescript": "4.5.0-beta"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.18.0",
    "yarn": "3.0.2"
  },
  "env": {
    "optional": [],
    "required": []
  },
  "packageManager": "yarn@3.0.2"
}
