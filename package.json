{
  "name": "@flex-development/tutils",
  "description": "TypeScript utilities",
  "version": "3.1.0",
  "keywords": [
    "typescript-utilities",
    "typescript",
    "type-definitons"
  ],
  "homepage": "https://github.com/flex-development/tutils",
  "repository": "git://github.com/flex-development/tutils.git",
  "bugs": {
    "url": "https://github.com/flex-development/tutils/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevlopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./dist",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules || true",
    "postinstall": "husky-run install",
    "check:style": "eslint . --ext md,js,ts",
    "fix:format": "prettier --write .",
    "fix:style": "yarn check:style --fix --cache",
    "test": "zsh scripts/jest.sh",
    "compile": "zsh scripts/compile.sh",
    "release": "zsh scripts/release.sh",
    "release:github": "node scripts/js/release-github"
  },
  "devDependencies": {
    "@babel/core": "7.15.4",
    "@babel/eslint-parser": "7.15.4",
    "@commitlint/cli": "13.0.0",
    "@commitlint/config-conventional": "13.0.0",
    "@commitlint/format": "13.0.0",
    "@flex-development/grease": "1.1.0",
    "@types/debug": "latest",
    "@types/mvdan-sh": "0.5.0",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "@zerollup/ts-transform-paths": "latest",
    "debug": "latest",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.0.8",
    "eslint-plugin-markdown": "2.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "husky": "4.3.8",
    "lint-staged": "latest",
    "prettier": "2.3.2",
    "prettier-plugin-sh": "0.7.0",
    "replace-in-file": "latest",
    "ts-node": "latest",
    "ttypescript": "latest",
    "typescript": "4.2.4",
    "underscore-cli": "latest"
  },
  "peerDependencies": {
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": "use-yarn",
    "yarn": "3.0.1"
  },
  "env": {
    "optional": [
      "DEBUG",
      "DEBUG_COLORS"
    ],
    "required": [
      "NPM_TOKEN_FLDV",
      "PAT_GPR_FLDV"
    ]
  },
  "packageManager": "yarn@3.0.1",
  "release": {
    "allowBranch": "main"
  }
}
