{
  "name": "@flex-development/tutils",
  "description": "TypeScript utilities",
  "version": "3.1.0",
  "keywords": [
    "typescript-utilities",
    "typescript",
    "type-definitons"
  ],
  "homepage": "https://github.com/flex-development/tutils",
  "repository": "git://github.com/flex-development/tutils.git",
  "bugs": {
    "url": "https://github.com/flex-development/tutils/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevlopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./dist",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules || true",
    "postinstall": "husky-run install",
    "format": "prettier --write \"./\"",
    "lint": "eslint . --ext js,ts --fix --cache",
    "test": "zsh scripts/jest.sh",
    "compile": "zsh scripts/compile.sh",
    "release": "zsh scripts/release.sh",
    "release:github": "node scripts/js/release-github"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/eslint-parser": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@commitlint/format": "latest",
    "@types/debug": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@zerollup/ts-transform-paths": "latest",
    "debug": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-spellcheck": "latest",
    "husky": "4.3.8",
    "lint-staged": "latest",
    "prettier": "2.2.1",
    "replace-in-file": "latest",
    "standard-version": "latest",
    "ts-node": "latest",
    "ttypescript": "latest",
    "typescript": "4.2.4",
    "underscore-cli": "latest"
  },
  "peerDependencies": {
    "typescript": "4.2.4"
  },
  "engines": {
    "npm": "use-yarn",
    "yarn": "3.0.1"
  },
  "env": {
    "optional": [],
    "required": [
      "NPM_TOKEN_FLDV",
      "PAT_GPR_FLDV"
    ]
  },
  "packageManager": "yarn@3.0.1",
  "release": {
    "allowBranch": "main"
  }
}
