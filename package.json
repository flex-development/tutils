{
  "name": "@flex-development/tutils",
  "description": "TypeScript utilities",
  "version": "4.0.0-dev.0",
  "keywords": [
    "type definitons",
    "typescript utilities",
    "typescript"
  ],
  "homepage": "https://github.com/flex-development/tutils",
  "repository": "git://github.com/flex-development/tutils.git",
  "bugs": {
    "url": "https://github.com/flex-development/tutils/issues"
  },
  "license": "BSD-3-Clause",
  "contributors": [
    {
      "name": "Lexus Drumgold",
      "url": "https://github.com/unicornware"
    }
  ],
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "cjs",
    "esm",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.cjs"
    },
    "./cjs/*": "./cjs/*.cjs",
    "./esm/*": "./esm/*.mjs",
    "./package.json": "./package.json",
    "./types": {
      "import": "./esm/types/index.mjs",
      "require": "./cjs/types/index.cjs"
    },
    "./types/*": {
      "import": "./esm/types/*.mjs",
      "require": "./cjs/types/*.cjs"
    }
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "scripts": {
    "clean:build": "rimraf ./(cjs|esm)",
    "clean:modules": "rimraf ./node_modules",
    "clean:yarn": "rimraf .yarn/cache .yarn/*.gz",
    "clean": "concurrently \"yarn:clean:*\"",
    "bootstrap:husky": "chmod +x .husky/commit-msg .husky/pre-commit",
    "postinstall": "is-ci || yarn bootstrap:husky && husky install",
    "check:dedupe": "yarn dedupe --check",
    "check:format": "prettier --check .",
    "check:install": "yarn dlx @yarnpkg/doctor .",
    "check:style": "eslint . --ext cjs,md,js,ts",
    "check:upgrades": "yarn upgrade-interactive",
    "fix:format": "prettier --write .",
    "fix:style": "yarn check:style --fix --cache",
    "test": "echo no tests specified && exit 0",
    "build": "node ./tools/cli/build.ts",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "postpack": "toggle-scripts +postinstall",
    "release": "node ./tools/cli/release.ts",
    "prepublishOnly": "toggle-scripts -prepack",
    "postpublish": "toggle-scripts +prepack"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/eslint-parser": "7.15.7",
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/format": "13.2.0",
    "@commitlint/types": "13.2.0",
    "@flex-development/grease": "2.0.0",
    "@flex-development/log": "3.0.0-dev.0",
    "@types/fs-extra": "9.0.13",
    "@types/lodash.merge": "4.6.6",
    "@types/mvdan-sh": "0.5.1",
    "@types/node": "16.0.0",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.9",
    "@types/yargs": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vates/toggle-scripts": "1.0.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "chalk": "4.1.2",
    "concurrently": "6.3.0",
    "convert-extension": "0.3.0",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-markdownlint": "0.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "eslint-plugin-unicorn": "36.0.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "lint-staged": "11.2.0",
    "lodash.merge": "4.6.2",
    "prettier": "2.4.1",
    "prettier-plugin-sh": "0.7.1",
    "read-pkg": "7.0.0",
    "renamer": "4.0.0",
    "replace-in-file": "6.2.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "ttypescript": "1.5.12",
    "typescript": "4.4.3",
    "yargs": "17.2.1"
  },
  "peerDependencies": {
    "@types/node": ">=12.20.0",
    "typescript": ">=2.7"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=12.20.0",
    "yarn": "3.0.2"
  },
  "env": {
    "optional": [],
    "required": []
  },
  "packageManager": "yarn@3.0.2"
}
